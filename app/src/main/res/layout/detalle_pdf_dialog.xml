                        android:textColor="@color/colorPrimarymasDark"
                        android:textSize="18sp"
                        android:visibility="gone"
                        app:layout_constraintStart_toStartOf="@+id/textView22p"
                        app:layout_constraintTop_toBottomOf="@+id/view11p"
                        tools:visibility="visible" />

                    <TextView
                        android:id="@+id/razonIdp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:text="Nombre Documento"
                        android:textSize="18sp"
                        android:visibility="gone"
                        app:layout_constraintStart_toEndOf="@+id/razonTituloIdp"
