                    <TextView
                        android:id="@+id/textView19p"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        android:text="Email:"
                        android:textColor="@color/colorPrimarymasDark"
                        android:textSize="18sp"
                        app:layout_constraintStart_toStartOf="@+id/textView16p"
                        app:layout_constraintTop_toBottomOf="@+id/view3p" />

                    <TextView
                        android:id="@+id/textView21p"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        android:text="Sexo:"
                        android:textColor="@color/colorPrimarymasDark"
                        android:textSize="18sp"
                        app:layout_constraintStart_toStartOf="@+id/textView19p"
